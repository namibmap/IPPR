<div class="col-lg-6 col-sm-6">
    <h2>Licenses</h2>
    <div class="input-group">
        <div class="input-group-btn">
            <button class="btn btn-default" type="submit">
                <i class="glyphicon glyphicon-search"></i>
            </button>
        </div>
        <input type="text" class="form-control" value="{{licenseType}}">
    </div>
    <ul class="list-group">
        <%  for (var property in licenses){ %>
            <li class="list-group-item list-header"><%= property %></li>
				<% for (var i=1;i<licenses[property].length;i++){ 				
					var license_id = "";
					concessionName = licenses[property][i].concession;
					license_id = "license_" + licenses[property][i].id;
				%>
            <li class="list-group-item list-block" data-ng-click="makeSelection($event, 'license')" id=<%= license_id %>> <%= concessionName %></li>
        <%}
        }%>
    </ul>
</div>