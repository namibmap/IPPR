<div class="col-lg-6 col-sm-6">
    <h2>Licenses</h2>
    <div class="input-group">
        <div class="input-group-btn">
            <button class="btn btn-default" type="submit">
                <i class="glyphicon glyphicon-search"></i>
            </button>
        </div>
        <input type="text" class="form-control">
    </div>
    <ul class="list-group">
        <%  for (var property in licenses){ %>
            <li class="list-group-item list-header"><%= property %></li>
            <% for (var i=0; i<licenses[property].length; i++){ %>
                <li class="list-group-item list-block"><%= licenses[property][i] %></li>
            <%}
        }%>
    </ul>
</div>