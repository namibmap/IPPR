<div class="col-lg-6 col-sm-6">
    <h2>Companies</h2>
    <div class="input-group">
        <div class="input-group-btn">
            <button class="btn btn-default" type="submit">
                <i class="glyphicon glyphicon-search"></i>
            </button>
        </div>
        <input type="text" class="form-control" id="filter-companies">
    </div>
    <ul class="list-group">
        <% for (var company_id in companies) { %>
            <li class="list-group-item license-company-name" id="<%= company_id %>"><%= companies[company_id] %></li>
        <% } %>
    </ul>
</div>